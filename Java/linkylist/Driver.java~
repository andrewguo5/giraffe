public class Driver {

    public static void testAdd(MyLinkedList turtle){
	for (int n = 0; n < 12; n++) {
	    turtle.add((new Integer((int)(Math.random()*24))), 0);
	}
	System.out.println(turtle);
    }

    public static void testRemove(MyLinkedList turtle) {
	for (int n = 0; n < 6; n++) {
	    turtle.remove(0);
	}	
	System.out.println(turtle);
    }

    public static void main (String[] args) {
	
	MyLinkedList test1 = new MyLinkedList();
	MyLinkedList test2 = new MyLinkedList();
	MyLinkedList test3 = new MyLinkedList();
	MyLinkedList test4 = new MyLinkedList();
	MyLinkedList test5 = new MyLinkedList();
	MyLinkedList test6 = new MyLinkedList();
	MyLinkedList test7 = new MyLinkedList();
	MyLinkedList test8 = new MyLinkedList();

	testAdd(test1);
	testAdd(test2);
	testAdd(test3);
	testAdd(test4);
	testAdd(test5);
	testAdd(test6);
	testRemove(test1);
	testRemove(test2);
	testRemove(test3);
	testRemove(test4);
	testRemove(test5);
	testRemove(test6);

    }
}
